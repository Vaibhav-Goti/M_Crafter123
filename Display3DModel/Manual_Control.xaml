        </Button>
        <TextBox x:Name="xvaluetxt" HorizontalAlignment="Left" Height="20" Margin="155,14,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="67" TextChanged="xvaluetxt_TextChanged"/>
